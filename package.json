    "@types/text-encoding": "^0.0.36",
    "axios": "^1.4.0",
    "daisyui": "^2.51.6",
    "next": "^13.1.2",
    "next-seo": "^5.15.0",
    "next-sitemap": "^3.1.45",
    "next-themes": "^0.2.1",
    "patch-package": "^7.0.0",
    "protobufjs": "^7.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
