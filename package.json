    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.14",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
